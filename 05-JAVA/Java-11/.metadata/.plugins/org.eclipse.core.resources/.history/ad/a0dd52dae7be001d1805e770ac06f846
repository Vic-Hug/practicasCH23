package com.vhfc.hilosRunn;

public class Hilo2 implements Runnable{
	
	private String nombre;
	


	public Hilo2(String nombre) {
		this.nombre = nombre;
	}

	public String getNombre() {
		return nombre;
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		for (int j = 0; j < 10; j++) {
			
			System.out.println(j + " - " + nombre);
		}
		
	}

}
